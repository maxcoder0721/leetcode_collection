   "source": [
    "class Solution(object):\n",
    "    def removeDuplicates(self, nums):\n",
    "        L = 0\n",
    "        R = 1\n",
    "        while R <= len(nums) - 1:\n",
    "            if nums[L] == nums[R]:\n",
    "                nums.pop(R)\n",
    "            else:\n",
    "                L = R\n",
    "                R += 1\n",
    "        \n",
    "                \n",
    "        return len(nums)\n",
    "            "
   ]
  }
 ],
